#: A_New_Order/scenarios/27.cfg:620 A_New_Order/scenarios/27.cfg:891
msgid "Death of Gawen Hagarthen"
msgstr "Gawen Hagarthen dör"

#: A_New_Order/scenarios/14macros.cfg:13 A_New_Order/scenarios/15b.cfg:102
#: A_New_Order/scenarios/8.cfg:176 A_New_Order/scenarios/7.cfg:296
#: A_New_Order/scenarios/12.cfg:217
msgid "Death of Karen"
msgstr "Karen dör"

#: A_New_Order/scenarios/19oracle.cfg:767 A_New_Order/scenarios/14macros.cfg:5
#: A_New_Order/scenarios/17.cfg:331 A_New_Order/scenarios/15b.cfg:94
