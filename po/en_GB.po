#: A_New_Order/scenarios/Breaking_The_Circle.cfg:497
#: A_New_Order/scenarios/15b.cfg:98
msgid "Death of Gawen Hagarthen"
msgstr ""

#: A_New_Order/scenarios/14macros.cfg:13 A_New_Order/scenarios/7.cfg:296
#: A_New_Order/scenarios/8.cfg:176 A_New_Order/scenarios/12.cfg:217
#: A_New_Order/scenarios/15b.cfg:102
msgid "Death of Karen"
msgstr ""

#: A_New_Order/scenarios/19a.cfg:153 A_New_Order/scenarios/19b.cfg:140
#: A_New_Order/scenarios/14macros.cfg:5 A_New_Order/scenarios/7.cfg:292
#: A_New_Order/scenarios/8.cfg:172 A_New_Order/scenarios/21haunted.cfg:147
#: A_New_Order/scenarios/21haunted.cfg:171
