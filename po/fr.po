#: A_New_Order/scenarios/27.cfg:617 A_New_Order/scenarios/27.cfg:884
msgid "Death of Gawen Hagarthen"
msgstr "Mort de Gawen Hagarthen"

#: A_New_Order/scenarios/14macros.cfg:13 A_New_Order/scenarios/15b.cfg:110
#: A_New_Order/scenarios/8.cfg:171 A_New_Order/scenarios/7.cfg:293
#: A_New_Order/scenarios/12.cfg:214
msgid "Death of Karen"
msgstr "Mort de Karen"

#: A_New_Order/scenarios/19oracle.cfg:766 A_New_Order/scenarios/14macros.cfg:5
#: A_New_Order/scenarios/17.cfg:328 A_New_Order/scenarios/15b.cfg:102
