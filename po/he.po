#: A_New_Order/scenarios/20.cfg:424 A_New_Order/scenarios/20.cfg:430
#: A_New_Order/scenarios/29.cfg:3
msgid "Lorin"
msgstr ""

#: A_New_Order/scenarios/13.cfg:546 A_New_Order/scenarios/13.cfg:554
msgid "Yaranov"
msgstr ""

#: A_New_Order/scenarios/13.cfg:547 A_New_Order/scenarios/13.cfg:555
#: A_New_Order/scenarios/13.cfg:564 A_New_Order/scenarios/13.cfg:570
