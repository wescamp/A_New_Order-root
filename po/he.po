#: A_New_Order/scenarios/20.cfg:323 A_New_Order/scenarios/20.cfg:371
msgid "Please, tell me..."
msgstr ""

#: A_New_Order/scenarios/13.cfg:436 A_New_Order/scenarios/13.cfg:476
#: A_New_Order/scenarios/13.cfg:509
msgid "Do you know where the place called Bontom is?"
msgstr ""

#: A_New_Order/scenarios/13.cfg:437 A_New_Order/scenarios/13.cfg:449
#: A_New_Order/scenarios/13.cfg:477 A_New_Order/scenarios/13.cfg:489
#: A_New_Order/scenarios/13.cfg:510 A_New_Order/scenarios/13.cfg:522
msgid "Do you know anything about Reme Carrenemoe?"
msgstr ""

#: A_New_Order/scenarios/13.cfg:438 A_New_Order/scenarios/13.cfg:450
#: A_New_Order/scenarios/13.cfg:478 A_New_Order/scenarios/13.cfg:490
#: A_New_Order/scenarios/13.cfg:511 A_New_Order/scenarios/13.cfg:523
msgid "Have you heard anything about orcs?"
msgstr ""

#: A_New_Order/scenarios/13.cfg:439 A_New_Order/scenarios/13.cfg:451
#: A_New_Order/scenarios/13.cfg:479 A_New_Order/scenarios/13.cfg:491
#: A_New_Order/scenarios/13.cfg:512 A_New_Order/scenarios/13.cfg:524
msgid "Would you want to fight for me?"
msgstr ""

#: A_New_Order/scenarios/13.cfg:440 A_New_Order/scenarios/13.cfg:452
#: A_New_Order/scenarios/13.cfg:480 A_New_Order/scenarios/13.cfg:492
#: A_New_Order/scenarios/13.cfg:513 A_New_Order/scenarios/13.cfg:525
msgid "Go and work, good man."
msgstr ""

#: A_New_Order/scenarios/13.cfg:464
msgid "Ah, welcome, we've heard about your victories, Gaumhaldric."
msgstr ""

#: A_New_Order/scenarios/13.cfg:466
msgid "I still can't get used that they are calling him Gaumhaldric."
msgstr ""

#: A_New_Order/scenarios/13.cfg:467
msgid "Cute name, isn't it?"
msgstr ""

#: A_New_Order/scenarios/13.cfg:470
msgid "Everything for you, young knight."
msgstr ""

#: A_New_Order/scenarios/13.cfg:504
msgid "Free men from Raedwood forest! You are always welcomed here."
msgstr ""

#: A_New_Order/scenarios/13.cfg:541 A_New_Order/scenarios/13.cfg:550
#: A_New_Order/scenarios/13.cfg:560 A_New_Order/scenarios/13.cfg:567
#: A_New_Order/scenarios/18.cfg:528 A_New_Order/scenarios/18.cfg:535
